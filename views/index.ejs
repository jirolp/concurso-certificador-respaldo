<div class="container mb-5">
    <div class="">
        <div class="d-flex justify-content-center">
            <h1 class=" title-index text-center">Algunos Productos</h1>
        </div>

        <div class="d-flex justify-content-center">
            <p class="logo">"Nunca eres Demasiado<br>viejo para un Juguete"</p>

        </div>

        
    </div>
    
</div>
<div class="container mb-4">
    <div id="demo" class="carousel slide" data-ride="carousel">

        <!-- Indicators -->
        <ul class="carousel-indicators">
            <li data-target="#demo" data-slide-to="0" class="active"></li>
            <li data-target="#demo" data-slide-to="1"></li>
            <li data-target="#demo" data-slide-to="2"></li>
        </ul>

        <!-- The slideshow -->
        <div class="carousel-inner container-fluid justify-content-center">
            <div class="carousel-item active">
                <img class="container-fluid justify-content-center" src="fotos/juguetes_1.jpg" alt="Los Angeles">
            </div>
            <div class="carousel-item">
                <img class="container-fluid justify-content-center" src="fotos/juguetes_2.jpg" alt="Chicago">
            </div>
            <div class="carousel-item">
                <img class="container-fluid justify-content-center" src="fotos/juguetes_3.jpg" alt="New York">
            </div>
        </div>

        <!-- Left and right controls -->
        <a class="carousel-control-prev" href="#demo" data-slide="prev">
            <span class="carousel-control-prev-icon"></span>
        </a>
        <a class="carousel-control-next" href="#demo" data-slide="next">
            <span class="carousel-control-next-icon"></span>
        </a>
    </div>
</div>
